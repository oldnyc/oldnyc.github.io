{
  "name": "oldnyc",
  "version": "1.0.0",
  "description": "The oldnyc.org frontend",
  "main": "js/viewer.js",
  "scripts": {
    "test": "npm run lint",
    "format": "yarn prettier . --write",
    "format:check": "yarn prettier . --check",
    "lint": "eslint src",
    "serve": "webpack serve",
    "build": "webpack --mode production --no-devtool && rm -rf bundle && cp -r dist/* .",
