    "html-webpack-plugin": "^5.6.0",
    "knip": "^5.27.2",
    "prettier": "3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "rc-slider": "^11.1.5",
    "react": "^18.3.1",
